{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/public",
  "publicPath": "/",
  "framework": "other",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**": {
      "runtime": "nodejs20.x"
    }
  }
}
